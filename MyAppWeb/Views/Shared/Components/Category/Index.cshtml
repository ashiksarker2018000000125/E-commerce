@model List<MyApp.Models.Category>

<li class="nav-item dropdown">
    <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Phone Brands</a>
    <div class="dropdown-menu">
        @foreach (var item in Model)
        {
            <a class="dropdown-item" asp-action="Index" asp-controller="CategoryProduct" asp-area="Customer" asp-route-id="@item.Id">@item.Name</a>
        }
    </div>
</li>
